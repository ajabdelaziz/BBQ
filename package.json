{
  "name": "bbq",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "babel-jest": "^23.6.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "envify": "^4.1.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-react": "^6.24.1"
  },
  "scripts": {
    "build-jsx": "scripts/build-jsx"
  },
  "eslintIgnore": [
    "**/lib/*.js",
    "*.min.js"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "prettier/prettier": "error",
      "linebreak-style": [
        "error",
        "unix"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        0,
        {
          "vars": "local",
          "args": "none"
        }
      ],
      "no-console": 0,
      "no-case-declarations": 0
    },
    "globals": {
      "Grid": false,
      "Cell": false,
      "React": false
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
